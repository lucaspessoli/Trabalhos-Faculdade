programa {
	funcao inicio() {
		
		cadeia nome, telefone, endereco
		
		escreva("Informe o Nome: ")
		leia(nome)
		
		escreva("Informe o telefone: ")
		leia(telefone)
		
		escreva("Informe o Endereço:")
		leia(endereco)
		
		escreva("Nome: " + nome," Telefone: " + telefone," Endereço: " + endereco)
	}
}
